library(dplyr)
library(readr)

# Read all CSVs as characters to avoid mismatched types
merged_data <- list.files(pattern = "*.csv", full.names = TRUE) %>%
  lapply(function(file) read_csv(file, col_types = cols(.default = "c"))) %>%
  bind_rows()

# Save the merged data
write_csv(merged_data, "merged_output.csv")
